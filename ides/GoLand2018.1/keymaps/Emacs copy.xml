<keymap version="1" name="Emacs copy" parent="Emacs">
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="alt 1" />
    <keyboard-shortcut first-keystroke="ctrl comma" second-keystroke="f" />
  </action>
  <action id="ActivateStructureToolWindow">
    <keyboard-shortcut first-keystroke="alt 7" />
    <keyboard-shortcut first-keystroke="ctrl comma" second-keystroke="s" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl t" second-keystroke="t" />
  </action>
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="shift alt f10" />
    <keyboard-shortcut first-keystroke="meta r" />
  </action>
  <action id="EditorCodeBlockEnd">
    <keyboard-shortcut first-keystroke="ctrl alt close_bracket" />
  </action>
  <action id="EditorCodeBlockStart">
    <keyboard-shortcut first-keystroke="ctrl alt open_bracket" />
  </action>
  <action id="EditorLineEnd">
    <keyboard-shortcut first-keystroke="end" />
  </action>
  <action id="FindInPath">
    <keyboard-shortcut first-keystroke="shift ctrl f" />
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="e" />
  </action>
  <action id="FindUsages">
    <keyboard-shortcut first-keystroke="alt f7" />
    <keyboard-shortcut first-keystroke="shift alt s" />
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="s" />
  </action>
  <action id="Go To Tab #1">
    <keyboard-shortcut first-keystroke="meta 1" />
  </action>
  <action id="Go To Tab #2">
    <keyboard-shortcut first-keystroke="meta 2" />
  </action>
  <action id="Go To Tab #3">
    <keyboard-shortcut first-keystroke="meta 3" />
  </action>
  <action id="Go To Tab #4">
    <keyboard-shortcut first-keystroke="meta 4" />
  </action>
  <action id="Go To Tab #5">
    <keyboard-shortcut first-keystroke="meta 5" />
  </action>
  <action id="Go To Tab #6">
    <keyboard-shortcut first-keystroke="meta 6" />
  </action>
  <action id="Go To Tab #7">
    <keyboard-shortcut first-keystroke="meta 7" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl alt g" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="period" />
    <keyboard-shortcut first-keystroke="alt period" />
    <mouse-shortcut keystroke="control button1" />
    <keyboard-shortcut first-keystroke="ctrl close_bracket" />
  </action>
  <action id="MethodUp">
    <keyboard-shortcut first-keystroke="alt up" />
    <keyboard-shortcut first-keystroke="ctrl alt a" />
    <keyboard-shortcut first-keystroke="meta open_bracket" />
  </action>
  <action id="NextOccurence">
    <keyboard-shortcut first-keystroke="ctrl alt down" />
    <keyboard-shortcut first-keystroke="ctrl meta n" />
  </action>
  <action id="PreviousOccurence">
    <keyboard-shortcut first-keystroke="ctrl alt up" />
    <keyboard-shortcut first-keystroke="ctrl meta p" />
  </action>
  <action id="RecentFiles">
    <keyboard-shortcut first-keystroke="ctrl e" />
  </action>
  <action id="SearchEverywhere">
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="t" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="ctrl meta f" />
  </action>
  <action id="Vcs.UpdateProject" />
</keymap>